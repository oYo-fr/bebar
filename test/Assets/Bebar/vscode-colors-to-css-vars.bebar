helpers:
  - ./Assets/Helpers/vscode-colors-to-css-vars-helpers.js

templates:
  -
    file: ./Assets/Templates/vscode-colors-to-css-vars.hbs
    output: ./Assets/TestOutputs/vs-code-monokai-color-theme.css
    prettify:
      parser: css
    data:
      -
        url: https://raw.githubusercontent.com/microsoft/vscode/main/extensions/theme-monokai/themes/monokai-color-theme.json
        name: theme
        parseAs: raw
